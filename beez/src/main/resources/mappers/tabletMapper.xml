  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.sinc.beez.mapper.tablet">

	<select id="seat" parameterType="seat" resultType="seat">
		select SEAT_USEABLE_STATE 	
		from seat_tb 
	</select>
	
	<select id="userseat" parameterType="seat" resultType="userseat">
		select USER_LEAVE_YN
		from  USER_SEAT_TB 
	</select>

</mapper>
